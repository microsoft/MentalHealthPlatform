{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "moduleResolution": "node",
        "module": "CommonJS",
        "target": "es5",
        "lib": ["es5", "es6", "dom"],
        "jsx": "react",
        "allowJs": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
    },
    "include": [
        "./src/**/*",
        "./App.jsx"
    ]
}